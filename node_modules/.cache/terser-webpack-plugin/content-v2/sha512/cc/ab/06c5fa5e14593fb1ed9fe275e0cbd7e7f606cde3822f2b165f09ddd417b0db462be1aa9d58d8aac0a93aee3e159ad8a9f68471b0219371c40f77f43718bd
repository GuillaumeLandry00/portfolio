 { useState } from 'react'\r\nimport ArrowBackIosIcon from '@material-ui/icons/ArrowBackIos';\r\nimport ArrowForwardIosIcon from '@material-ui/icons/ArrowForwardIos';\r\n\r\n\r\nexport default function Carousel({ img }) {\r\n\r\n    const [current, setCurrent] = useState(0)\r\n    const length = img.length\r\n\r\n    const nextSlide = () => {\r\n        setCurrent(current === length - 1 ? 0 : current + 1);\r\n    }\r\n\r\n    const prevSlide = () => {\r\n        setCurrent(current === 0 ? length - 1 : current - 1);\r\n    }\r\n\r\n    if (!Array.isArray(img) || img.length <= 0) {\r\n        return null\r\n    }\r\n\r\n\r\n    return (\r\n        <section className='carousel'>\r\n            <ArrowBackIosIcon className='left-arrow' onClick={prevSlide} />\r\n            <ArrowForwardIosIcon className='right-arrow' onClick={nextSlide} />\r\n            {img.map((item, index) => {\r\n                return (\r\n                    <div className={index === current ? 'slide active' : 'slide'} key={index}>\r\n                        {index === current && (<img src={item} alt='app' />)}\r\n                    </div>\r\n                )\r\n            }\r\n            )}\r\n        </ section>\r\n    )\r\n}\r\n","import './showcase.scss';\r\nimport Carousel from '../carousel/Carousel';\r\n\r\nexport default function Showcase({ data }) {\r\n    console.log(data.img);\r\n    return (\r\n        <>\r\n            <h1 className='showCaseTitle'>{data.title}</h1>\r\n            <div className='showCase'>\r\n                <div className='left'>\r\n                    <Carousel img={data.img} />\r\n                </div>\r\n                <div className=\"right\">\r\n                    <h2 className='hide'>Détails</h2>\r\n                    <hr />\r\n                    <p>{data.details}</p>\r\n                    <div className='flex'>\r\n                        <div className='bottom'>\r\n                            <h3 className='title'>Téchnologies utilisées</h3>\r\n     